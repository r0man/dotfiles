Config { 
         font = "xft:Inconsolata-9"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
       , commands = [ 
          Run Weather "EDDT" ["-t","<station>: <tempC>Â°C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
         , Run Network "eth1" ["-L","0","-H","32","--normal","green","--high","red"] 10
         , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
         , Run Memory ["-t","Mem: <usedratio>"] 10
         , Run Battery ["-t","Battery: <left>"] 10
    	 , Run Date "%a %b %_d, %H:%M:%S" "date" 10
         , Run StdinReader
         , Run Swap [] 10
         ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %cpu% | %memory% * %swap% | %battery% | %eth1% | %EDDT% | <fc=#ffffff>%date%</fc>   "
}
